<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<Pane prefHeight="408.0" prefWidth="314.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.app.app.settings.Ics">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="408.0" prefWidth="314.0" spacing="10.0">
         <children>
            <Label contentDisplay="CENTER" prefHeight="35.0" prefWidth="500.0" text="Import ics calendar" textFill="WHITE" underline="true">
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </Label>
            <Button fx:id="ImportButton" mnemonicParsing="false" onAction="#importIcs" prefHeight="34.0" prefWidth="63.0" text="Import" />
            <Label layoutY="20.0" />
            <Label prefHeight="35.0" prefWidth="500.0" text="Save calendar to ics" textFill="WHITE" underline="true">
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </Label>
            <Button fx:id="SaveButton" mnemonicParsing="false" onAction="#saveIcs" prefHeight="34.0" prefWidth="63.0" text="Save" />
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </VBox>
   </children>
</Pane>
